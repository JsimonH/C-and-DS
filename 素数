#include<stdio.h>
#include<stdlib.h>
                  //编写一个关于100-200之间素数的c语言程序
 int main(){
  //素数.指在一个大于1的自然数中，除了1和此整数自身外，没法被其他自然数整除的数
  int i =0;
  for(i=100;i<=200;i++)  //首先将数的范围规划在100-200之间 
  {
   int j=0;
   for(j=2;j<i;j++)   //依次用2-199之间的数来与i进行模运算 
   {
   
    if(i%j==0)    //如果可以整数，说明不是素数 
     break;
    
    
   }
    if(j == i)   //如果最后没有任何一个数可以被j整数，说明这个i是素数 
     printf("%d ",i);
  }
   system("pause");
  return 0;
  
 } 

//函数方法判断素数
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int is_prime(int n)
{
	int i = 0;
	for (i = 2; i < n; i++)
	{
		if (n%i == 0)
		{
			return 0;
		}
	}
	return 1;
}
int main()
{
	int ret = 0;
	int num = 0;
	printf("输入一个数:\n");
	scanf("%d", &num);
	ret = is_prime(num);
	if (ret == 1)
	{
		printf("%d是素数\n", num);
	}
	if (ret==0)
	printf("%d不是素数\n", num);
	return 0;
}
